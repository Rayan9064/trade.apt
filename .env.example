# Environment variables for Trade.apt
# Copy this file to .env and fill in your values

# OpenAI API Key (optional - uses mock parser if not provided)
OPENAI_API_KEY=your_openai_api_key_here

# Groq API Key (optional - for AI agent)
GROQ_API_KEY=your_groq_api_key_here

# AI Model Configuration
AI_MODEL=llama-3.3-70b-versatile
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2000

# Server configuration
HOST=0.0.0.0
PORT=8000

# Alert check interval in seconds
ALERT_CHECK_INTERVAL=10

# Database path
DATABASE_PATH=data/tradeapt.db
