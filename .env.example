# ===========================================
# TradeAPT Environment Configuration
# ===========================================
# Copy this file to .env and fill in your values

# ----- AI Configuration -----
# OpenAI API Key (optional - uses mock parser if not provided)
OPENAI_API_KEY=your_openai_api_key_here

# Groq API Key (recommended - for AI agent)
GROQ_API_KEY=your_groq_api_key_here

# AI Model Configuration
AI_MODEL=llama-3.3-70b-versatile
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2000

# ----- Aptos Configuration -----
# Node URL (testnet/mainnet)
APTOS_NODE_URL=https://fullnode.testnet.aptoslabs.com/v1

# Private key for backend transactions (keep secret!)
APTOS_PRIVATE_KEY=your_private_key_here

# ----- Server Configuration -----
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Alert check interval in seconds
ALERT_CHECK_INTERVAL=10

# ----- Database -----
DATABASE_PATH=data/tradeapt.db

# ----- External APIs -----
# CoinGecko API (optional, for price data)
COINGECKO_API_KEY=

# ----- Frontend URLs (for CORS) -----
FRONTEND_URL=https://trade-apt.vercel.app
ALLOWED_ORIGINS=https://trade-apt.vercel.app,http://localhost:3000
